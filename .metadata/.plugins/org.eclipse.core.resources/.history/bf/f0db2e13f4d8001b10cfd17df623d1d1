package com.sophos.semillero.ui;

import org.openqa.selenium.By;


import net.serenitybdd.screenplay.targets.Target;

public class AliExpressHomePage {
	
	public static final Target TXT_NAME_PRODUCTO = Target.the("Campo donde se coloca el producto").locatedBy("//input[@name='SearchText']");
	public static final Target BTN_CERRAR_IMAG = Target.the("Boton para cerrar imagen ").located(By.xpath("//img[@src='https://ae01.alicdn.com/kf/H5545b1dae97243fba06c33bd33104b84G.png']"));
	public static final Target BTN_BUSCAR = Target.the("Boton para buscar producto").located(By.xpath("//input[@class='search-button']"));
}
